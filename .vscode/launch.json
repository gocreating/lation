    "version": "0.2.0",
    "configurations": [
        {
            "name": "Encrypt File",
            "type": "python",
            "request": "launch",
            "program": "${workspaceRoot}/lation.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceRoot}",
            "args": [
                "encrypt",
                "--src",
                "README.md",
                "--dest",
                "README.encrypted.md",
                "--password",
                "test",
            ]
        },
        {
            "name": "Decrypt File",
            "type": "python",
            "request": "launch",
            "program": "${workspaceRoot}/lation.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceRoot}",
            "args": [
                "decrypt",
                "--src",
                "README.encrypted.md",
                "--dest",
                "README.md",
                "--password",
                "test",
            ]
        },
        {
            "name": "Export Data",
            "type": "python",
            "request": "launch",
            "program": "${workspaceRoot}/lation.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceRoot}",
            "args": [
                "export-data",
                "--host",
                "h",
                "--username",
                "u",
                "--password",
                "p",
                "--database",
                "d",
            ]
        },
        {
            "name": "Import Data",
            "type": "python",
            "request": "launch",
            "program": "${workspaceRoot}/lation.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceRoot}",
            "args": [
                "import-data",
                "--host",
                "h",
                "--username",
                "u",
                "--password",
                "p",
