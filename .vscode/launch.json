    "version": "0.2.0",
    "configurations": [
        {
            "name": "Encrypt File",
            "type": "python",
            "request": "launch",
            "program": "${workspaceRoot}/lation.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceRoot}",
            "args": [
                "encrypt",
                "--src",
                "README.md",
                "--dest",
                "README.encrypted.md",
                "--password",
                "test",
            ]
        },
        {
            "name": "Decrypt File",
            "type": "python",
            "request": "launch",
            "program": "${workspaceRoot}/lation.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceRoot}",
            "args": [
                "decrypt",
                "--src",
                "README.encrypted.md",
                "--dest",
                "README.md",
                "--password",
                "test",
            ]
        },
        {
            "name": "[DB] Export",
            "type": "python",
            "request": "launch",
            "program": "${workspaceRoot}/lation.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceRoot}",
            "env": {
                "APP": "combo",
            },
            "args": [
                "db",
                "--url",
                "mysql+pymysql://user:pass@host:port/db",
                "export",
            ]
        },
        {
            "name": "[DB] Reset",
            "type": "python",
            "request": "launch",
            "program": "${workspaceRoot}/lation.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceRoot}",
            "env": {
                "APP": "combo",
            },
            "args": [
                "db",
                "--url",
                "mysql+pymysql://user:pass@host:port/db",
                "--model-agnostic",
                "reset",
            ]
        },
        {
            "name": "[Google Drive Storage] Sample List",
            "type": "python",
            "request": "launch",
            "program": "${workspaceRoot}/lation.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceRoot}",
            "justMyCode": false,
            "args": [
                "google-drive",
                "--credential-path",
                "./secrets/google-api-keys/lation-drive.json",
                "list",
                "--name",
                "combo-db-backup",
            ]
        },
        {
            "name": "[Google Drive Storage] Sample Create Dir",
            "type": "python",
            "request": "launch",
            "program": "${workspaceRoot}/lation.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceRoot}",
            "justMyCode": false,
            "args": [
                "google-drive",
                "--credential-path",
                "./secrets/google-api-keys/lation-drive.json",
                "create-dir",
                "--name",
                "combo-db-backup/2020_10_03_12_07_00",
            ]
        },
        {
            "name": "[Google Drive Storage] Sample Delete Dir",
            "type": "python",
            "request": "launch",
            "program": "${workspaceRoot}/lation.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceRoot}",
            "justMyCode": false,
            "args": [
                "google-drive",
                "--credential-path",
                "./secrets/google-api-keys/lation-drive.json",
                "delete-dir",
                "--name",
                "combo-db-backup",
            ]
        },
        {
            "name": "[Google Drive Storage] Sample Upload File",
            "type": "python",
            "request": "launch",
            "program": "${workspaceRoot}/lation.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceRoot}",
            "justMyCode": false,
            "args": [
                "google-drive",
                "--credential-path",
                "./secrets/google-api-keys/lation-drive.json",
                "upload-file",
                "--local",
                "exported-data/users.csv",
                "--remote",
                "combo-db-backup",
            ]
        },
        {
            "name": "[Google Drive Storage] Sample Upload Dir",
            "type": "python",
            "request": "launch",
            "program": "${workspaceRoot}/lation.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceRoot}",
            "justMyCode": false,
            "args": [
                "google-drive",
                "--credential-path",
                "./secrets/google-api-keys/lation-drive.json",
                "upload-dir",
                "--local",
                "exported-data",
                "--remote",
                "combo-db-backup",
            ]
        },
        {
            "name": "[Google Drive Storage] Sample Download File",
            "type": "python",
            "request": "launch",
            "program": "${workspaceRoot}/lation.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceRoot}",
            "args": [
                "google-drive",
                "--credential-path",
                "./secrets/google-api-keys/lation-drive.json",
                "download-file",
                "--remote",
                "combo-db-backup/users.csv",
                "--local",
                "download-data",
            ]
        },
        {
            "name": "[Google Drive Storage] Sample Download Dir",
            "type": "python",
            "request": "launch",
            "program": "${workspaceRoot}/lation.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceRoot}",
            "args": [
                "google-drive",
                "--credential-path",
                "./secrets/google-api-keys/lation-drive.json",
                "download-dir",
                "--remote",
                "combo-db-backup",
                "--local",
                "download-data",
            ]
        },
    ]
}
